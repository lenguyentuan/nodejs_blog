<div class="row mt-4">
    {{#each courses}}
    <div class="col-sm-6 col-lg-4">
        <div class="card" />
            <img class="card-img-top" src="{{this.img}}" alt="Card image cap">
            <div class="card-body">
                <h5 class="card-title">{{this.name}}</h5>
                <p class="card-text">{{this.description}}</p>
                <a href="#" class="btn btn-primary">Buy now</a>
            </div>
        </div>
    </div>
    {{/each}}
</div>